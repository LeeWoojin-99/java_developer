package Product;

public class Product {
	
	private String menu;
	private int price;
	private int amount = 0;
	
	public Product() {}
	public Product(String menu, int price) {
		this.menu = menu;
		this.price = price;
	}
	
	// getter setter
	public String getMenu() {
		return menu;
	}
	public void setMenu(String menu) {
		this.menu = menu;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
}
