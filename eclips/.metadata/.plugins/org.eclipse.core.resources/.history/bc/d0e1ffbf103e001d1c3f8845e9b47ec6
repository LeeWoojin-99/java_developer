package Day0927;

import java.util.ArrayList;

public class Collection01 {
	public static void main(String[] args) {
		
		ArrayList<Integer> arrList = new ArrayList<Integer>();
		
		arrList.add(1);
		arrList.add(2);
		arrList.add(3);
		
//		System.out.println(arrList[0]);
		for(int i: arrList) {
			System.out.print(i+" ");
		}
		System.out.println();
		
	}
}
