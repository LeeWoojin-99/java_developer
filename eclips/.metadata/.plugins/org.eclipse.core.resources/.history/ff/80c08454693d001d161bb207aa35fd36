package Lotto;

import java.util.Scanner;

public class Management {
	Lotto lottoUser;
	
	public void createLotto(Scanner sc) {
		String inputStrArr[];
		System.out.println("로또 번호를 입력해주세요.");
		inputStrArr = sc.nextLine().split(" ");
		int inputIntArr[] = new int[inputStrArr.length];
		for(int i=0; i<inputStrArr.length; i++) {
			inputIntArr[i] = Integer.parseInt(inputStrArr[i]);
		}
		for(int i: inputIntArr) {
			System.out.print(i+" ");
		}
	}
	
	public void createLottoAuto() {
		
		System.out.println("로또 번호 생성이 완료되었습니다.");
	}
}
