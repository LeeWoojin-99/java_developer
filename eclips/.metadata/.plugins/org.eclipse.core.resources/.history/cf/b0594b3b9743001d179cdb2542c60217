package Day1004;

/*
seq 변수를 가지는 쓰레드 생성
*/
public class ThreadJoinEx extends Thread{
	int seq;
	public ThreadJoinEx(int seq) {
		this.seq = seq;
	}
	
	public static void main(String[] args) {
		
	}

	@Override
	public void run() {
		System.out.println(this.seq+" thread start");
		System.out.println(this.seq+" thread end");
	}
	
	
}
