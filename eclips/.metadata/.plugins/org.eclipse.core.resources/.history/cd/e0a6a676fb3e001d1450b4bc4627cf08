package Day0928;

import java.util.HashMap;

public class Map05 {
	public static void main(String[] args) {
		HashMap<String, Integer> map1 = method1();
		
		HashMap<String, Integer> map2 = new HashMap<>();
		map2 = method2(map2);
		
		System.out.println(map1.entrySet());
		System.out.println(map2.entrySet());
		
		
		
	}
	
	static HashMap<String, Integer> method1() {
		// map에 상품 이름과 값을 추가
		HashMap<String, Integer> map = new HashMap<>();
		map.put("A", 1);
		map.put("B", 2);
		map.put("C", 3);
		map.put("D", 4);
		return map;
	}
	static HashMap<String, Integer> method2(HashMap<String, Integer> map) {
		// map에 상품 이름과 값을 추가
		map.put("A", 1);
		map.put("B", 2);
		map.put("C", 3);
		map.put("D", 4);
		return map;
	}
}
