package Lotto;

public class Lotto {
	int lottoArr[] = new int[6];
	
	public void Lotto() {
		for(int i=0; i<lottoArr.length; i++) {
			lottoArr[i] = (int)(Math.random()*9)+1;
		}
	}
	
	public boolean checkDuplication(int arr[], int num) {
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == num) {
				return false;
			}else if(arr[i] == 0) {
				return true;
			}
		}
		return true;
	}
}
