package Day1004;

import java.io.File;

public class FileObject {
	public static void main(String[] args) {
		File f = new File("D:\\\\이우진\\\\java_developer\\\\eclips\\\\Java_Project\\out.txt");
		System.out.println(f.getAbsolutePath().substring(0, 1));
		System.out.println(f.getAbsolutePath());
		System.out.println(f.getName());
		for(String i: f.getName().split(".")) {
			System.out.println(i);
		}
	}
}
