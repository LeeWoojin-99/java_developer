package Day1005;

import java.util.StringTokenizer;

public class LWJ {
	public static void main(String[] args) throws Exception {
		
		StringBuffer sb = new StringBuffer();
		sb.append("a"); // a
		sb.append("b"); // ab
		sb.insert(1, "c"); // acb
		sb.delete(0, 1);
		System.out.println(sb);
		
	}
	
}
