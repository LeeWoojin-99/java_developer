package Day1004;

public class ThreadMain03 {
	public static void main(String[] args) {
		for(int i=10; i>0; i--) {
			Thread th = new Thread03("thread"+i);
			if(i == 10) {
				th.setPriority(Thread.MAX_PRIORITY);
			}else {
				th.setPriority(Thread.MIN_PRIORITY);
			}
			th.start();
		}
		System.out.println("main exit");
	}
}
