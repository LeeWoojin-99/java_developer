package Library;

import java.io.BufferedReader;
import java.util.ArrayList;

public interface ManagementIF {
	public void printMenu(); // 메뉴 출력
	
	public int selectBook(BufferedReader br); // 책 선택
	public void rentalBook(BufferedReader br); // 빌리기
	public void extension(BufferedReader br); // 연장
	public void returnBook(BufferedReader br); // 반납
	
	public void printBookList(); // 책 리스트 정보 조회
	public void printBook(BufferedReader br); // 책 한권의 정보
	
}
