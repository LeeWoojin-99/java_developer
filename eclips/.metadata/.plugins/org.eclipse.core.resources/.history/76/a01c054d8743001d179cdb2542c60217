package Day1004;


public class LWJ {
	public static void main(String[] args) {
		
		Thread t1 = new Thread(()->{
			for(int i=0; i<10; i++) System.out.println("thread : i");
			try {
				Thread.sleep(500);
			} catch (Exception e) {
				// TODO: handle exception
				e.printStackTrace();
			}
		});

		t1.start();
	}
}
