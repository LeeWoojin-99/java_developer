package Day1001;

import java.util.Arrays;

public class LWJ {
	public static void main(String[] args) {
		
		int arr[] = {3,1,2};
		
		int sum = Arrays.stream(arr);
		System.out.println(sum);
		
		
		lambda var = (a, b) -> a + b;
		System.out.println(var.plus(1, 2));
		
	}
}

interface lambda {
	public int plus(int a, int b);
}