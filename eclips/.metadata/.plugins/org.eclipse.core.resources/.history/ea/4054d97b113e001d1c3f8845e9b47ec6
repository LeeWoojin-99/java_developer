//		ArrayList<Integer> arrList = new ArrayList<Integer>();
		List arrList = new ArrayList(); // 모든 객체를 저장할 수 있다.
		
//		arrList.add(1);
//		arrList.add(2);
//		arrList.add(3);
		
		System.out.println(arrList.get(0));
