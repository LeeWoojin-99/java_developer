package Day0929;

import java.util.ArrayList;

public class LWJ {
	public static void main(String[] args) {
		ArrayList<Sale<String, Integer>> list = new ArrayList<>();
		
		list.add(new Sale<>("햄버거", 4000));
		list.add(new Sale<>("피자", 12000));
		list.add(new Sale<>("음료수", 1500));
		
		for(Sale<String, Integer> i: list) {
			System.out.printf("%s : %d원\n", i.getMenu(), i.getPrice());
		}
	}
}
