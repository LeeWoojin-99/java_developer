		EzenComputer s1 = new EzenComputer("홍길동", "24", "01011112222", "인천");
		EzenComputer s2 = new EzenComputer("김하나", "25", "01011113333", "인천");
		EzenComputer s3 = new EzenComputer("이둘", "26", "01011114444", "서울");
		EzenComputer s4 = new EzenComputer("최셋", "27", "01011115555", "대전");
		EzenComputer s5 = new EzenComputer("서넷", "28", "01011116666", "부산");
		s1.printStudent();
		s2.printStudent();
		s3.printStudent();
		s4.printStudent();
		s5.printStudent();
	}
}