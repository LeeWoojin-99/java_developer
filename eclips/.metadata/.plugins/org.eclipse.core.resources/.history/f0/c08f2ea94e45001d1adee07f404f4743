package Library;

import java.util.ArrayList;

public interface ManagementIF {
	ArrayList<Book> books;
	
	public void printMenu(); // 메뉴 출력
	
	public boolean selectBook(); // 책 선택
	public void rentalBook(); // 빌리기
	public void extension(); // 연장
	public void returnBook(); // 반납
	
	public void printBookList(); // 책 리스트 정보 조회
	public void printBook(); // 책 한권의 정보
	
}
