package Day1004;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Comparator;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class LWJ {
	public static void main(String[] args) {
		
		int data[] = {5,6,4,2,3,1,1,2,2,4,4,6,6,7};
		
		Arrays.stream(data) // make IntStream
		.boxed() // IntStream to Stream<Integer>
		.distinct().sorted(Comparator.reverseOrder()) // sort
		.forEach(i->System.out.println(i)); // print
		
		Arrays.stream(data) // make IntStream
		.boxed() // IntStream to Stream<Integer>
		.distinct().sorted(Comparator.reverseOrder()) // sort
		.mapToInt(Integer::intValue) // Stream<Integer> to IntStream
		.toArray(); // IntStream to int array

	}
}
