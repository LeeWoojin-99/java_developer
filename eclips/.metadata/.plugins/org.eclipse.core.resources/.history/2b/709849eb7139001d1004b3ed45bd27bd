interface Power{
	public abstract void turnOn();
	public abstract void turnOff();
	
}

class Tv1 implements Power{
	boolean power;
	
	public void turnOn() {
		power = true;
		System.out.println("on");
	}
	public void turnOff() {
		power = talse;
		System.out.println("off");
	}
}