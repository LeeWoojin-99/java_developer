package Day0920;

public class class04 {
	
}

class Car{
	private String color;
	private int door;
	private int airback;
	private boolean power;
	
	public Car(String color) {
		this.color = color;
	}
	public Car(String color, int door) {
		this.color = color;
		this.door = door;
	}
	public Car(String color, int door, int airback) {
		this(color, door);
		this.airback = airback;
	}
}