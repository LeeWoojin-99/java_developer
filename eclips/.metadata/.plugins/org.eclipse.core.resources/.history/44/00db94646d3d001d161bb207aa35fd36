package Day0926;

import java.util.Scanner;

public class LWJ {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		String input = sc.next();
		System.out.println("test"+input);
		
		String input2;
		System.out.println("입력");
		input2 = sc.nextLine();
		System.out.println(input2);
		
		sc.close();
	}
}
