package Day1004;

public class Thread03 extends Thread{
	public Thread03(String name) {
		setName(name); // thread의 이름을 바꾸기
	}

	@Override
	public void run() {
		for(int i=0; i<10000; i++) {
			
			try {
			} catch (Exception e) {
				// TODO: handle exception
				e.printStackTrace();
			}
		}
		System.out.println(getName());
	}
}
