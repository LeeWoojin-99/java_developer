package Day0928;

import java.util.TreeSet;

public class TreeSet02 {
	public static void main(String[] args) {
		int score[] = {80, 95, 78, 32, 56, 12, 98, 66};
		
		TreeSet<Integer> set = new TreeSet<>();
		for(int i: score) set.add(i);
		
		System.out.println(set);
		
		System.out.println(set.tailSet(80)); // 이상
		System.out.println(set.headSet(80)); // 미만
		
		TreeSet<String> set2 = new TreeSet<String>();
		set2.add("aaa"); set2.add("abc"); set2.add("abd"); set2.add("abe");
		set2.add("faa"); set2.add("gbc"); set2.add("hbc"); set2.add("acc");
		
		System.out.println(set2);
	}
}
