package Day0920;

public class class05 {
	public static void main(String[] args) {
		
	}
}

/*
 * TV Class
 * power
 * powerToggle
 * printPower
 * 
 * channel
 * chUp
 * chDown
 * 
 * volume
 * volUp
 * volDown
 * 
 * channel혹은 volume이 up down시 print
 * 
*/
TV{
	private boolean power;
	private int channel;
	private int volume;
	
	public void powerToggle() {
		if(power == true) {
			power = false;
		}else {
			power = true;
		}
	}
	
	public void chUp() {
		channel++;
		System.out.printf("현재 채널은 %d입니다.\n", channel);
	}
	public void chDown() {
		channel--;
		System.out.printf("현재 채널은 %d입니다.\n", channel);
	}
	public void volUp() {
		volume++;
		System.out.printf("현재 음량은 %d입니다.\n", channel);
	}
	public void volDown() {
		volume--;
		System.out.printf("현재 음량은 %d입니다.\n", channel);
	}
	
}