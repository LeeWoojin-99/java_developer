package Day1004;

import java.io.IOException;

public class FileWriter {
	public static void main(String[] args) throws IOException {
		java.io.FileWriter fw = new java.io.FileWriter("writer.txt");
		
		String data = "";
		for(int i=0; i<10; i++) {
			data+=i
		}
		fw.write(data);
		fw.close();
	}
}
