package Day0922;

public class Exception02 {
	public static void main(String[] args) {
		/*
		아래 코드들은 예외가 발생할 수 있는 코드들
		Exception
		*/
		
		try {
			// ArrayIndexOutOfBoundsException
//			int arr[] = new int [5];
//			for(int i=0; i<=arr.length; i++) {
//				System.out.println(arr[i]+' ');
//			}
			
			// ClassCastException
			P0 p = new P0();
			C0 c = (C0) p;
		} catch (Exception e) {
			e.printStackTrace();
			System.out.println("예외입니다");
		}
		
	}
}
class P0{
	int numP = 1;
}
class C0 extends P0{
	int numC = 2;
}